<Optimize>
  <Assets path="sample/assets.${YYYYMMDD}" bcost="0.0003" scost="0.0013" group_path="sample/group.${YYYYMMDD}">
    <Composite>
      <HS300 path="sample/HS300.${YYYYMMDD}"/>
      <CS500 path="sample/CS500.${YYYYMMDD}"/>
    </Composite>
  </Assets>
  <InitPortfolio path="sample/init_portfolio.${YYYYMMDD}"/>
  <Universe path="sample/universe.${YYYYMMDD}"/>
  <RiskModel path="/home/SambaServer/extend_data/Barra/short/${YYYY}/${MM}/${DD}" name="CNE5S"/>
  <Case risk_target="None">
    <Utility lambda_f="0.05" lambda_d="0.025" lambda_alpha="1" lambda_cost="1" lambda_penality="1" lambda_residual="0"/>
    <Constraints>
      <HedgeConstraints>
        <Leverage>
          <Short lower="-1" upper="-1"/>
        </Leverage>
      </HedgeConstraints>
      <ParingConstraints closeout="true">
        <Holding>
          <LongNum min="150" min_soft="false" max="400" max_soft="false"/>
        </Holding>
      </ParingConstraints>
      <LinearConstraints crossover="False">
        <AssetRange path="" white="" black="">
          <Regular lower="0.0" upper="0.03"/>
          <Composite>
            <HS300 lower="-1.0" upper="-1.0"/>
            <CS500 lower="-1.0" upper="-0"/>
          </Composite>
        </AssetRange>
        <Group path="">
          <Industry reference="HS300" relative="plus" soft="0">
            <Attrib value="480000" lower="-0.03" upper="0.04"/>
            <Attrib value="490000" lower="-0.04" upper="0"/>
          </Industry>
        </Group>
      </LinearConstraints>
    </Constraints>
  </Case>
  <Solver tolerance_multiplier="1" crossover_as_one="true" timeout="60" input="sample/input.${YYYYMMDD}" output="sample/output.${YYYYMMDD}" weight="sample/ideal_weight.${YYYYMMDD}">
    <Exposure factor="sample/factor_exposure.${YYYYMMDD}" group="sample/group_exposure.${YYYYMMDD}" expanded="sample/expaned_weight.${YYYYMMDD}"/>
  </Solver>
</Optimize>

